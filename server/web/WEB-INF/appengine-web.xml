<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>killardgame</application>

    <version>1</version>

    <static-files>
        <include path="/**.js" expiration="1h"/>
        <include path="/favicon.ico" expiration="1h"/>
        <!--<include path="/**.png" expiration="1d 1h"/>-->
        <include path="/**.gif" expiration="1h"/>
        <include path="/css/**.png"/>
        <include path="/image/**.png"/>
        <include path="/**.jpg"/>
        <include path="/**.css"/>
        <include path="/**.swf"/>
        <include path="/**.txt"/>
        <exclude path="/arena/**.png"/>
        <exclude path="/package/**.png"/>
        <exclude path="/**.html"/>
        <exclude path="/**.json"/>
        <exclude path="/**.xml"/>
    </static-files>

    <resource-files>
        <include path="/**.png"/>
        <include path="/**.json"/>
        <include path="/**.xml"/>
    </resource-files>

    <sessions-enabled>false</sessions-enabled>

    <inbound-services>
        <service>xmpp_message</service>
    </inbound-services>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties"/>
    </system-properties>

</appengine-web-app>
